{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":disableDependencyDashboard"],
  "stabilityDays": 5,
  "prCreation": "not-pending",
  "timezone": "UTC",
  "schedule": ["after 10am every weekday", "before 9pm every weekday"],
  "labels": ["dependencies"],
  "vulnerabilityAlerts": {
    "enabled": true,
    "labels": ["security"]
  },
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
